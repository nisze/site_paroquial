# EditorConfig - Configuração de encoding para o projeto
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
charset = utf-8
indent_style = space
indent_size = 4

[*.{xml,properties,yml,yaml}]
charset = utf-8
indent_style = space
indent_size = 2